#!/usr/bin/env bash
#
# Script to set colors generated by 'wal'
# https://github.com/dylanaraps/wal

# Source generated colors.
source "${HOME}/.cache/wal/colors.sh"

reload_dunst() {
    pkill dunst && \
        dunst \
            -lb "$color0" \
            -nb "$color0" \
            -cb "$color9" \
            -lf "$color15"  \
            -bf "$color15"  \
            -cf "$color15" &
}


main() {
    reload_dunst &
}

main >/dev/null 2>&1

